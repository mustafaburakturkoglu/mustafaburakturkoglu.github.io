c tab is clicked\n    $(cs_block_tabs_id + ' li a').click(function(){\n\n      \/\/ Get ID of the clicked tab\n      let tab_item_id = $(this).attr('id');\n\n      \/\/ If this tab is inactive, add the inactive class to all the other tabs, and remove the same class from this element.\n      if($(this).hasClass('inactive')){\n        $(cs_block_tabs_id + ' li a').addClass('inactive');\n        $(this).removeClass('inactive');\n\n        \/\/ Hide all tabs' contents, and show THIS tab's content\n        $(cs_block_tabs_content_id + ' .container').hide();\n        $('#'+ tab_item_id + 'C').fadeIn(300);\n     }\n    });\n\n  });\n})(jQuery);\n<\/script> \n\n\n<div class=\"wp-container-62756236013e2 wp-block-group alignfull dbio-use-cases\"><div class=\"wp-block-group__inner-container\">\n<div class=\"wp-block-genesis-blocks-gb-columns gb-layout-columns-1 one-column gb-has-custom-background-color alignfull\" style=\"padding-top:2em;background-color:#14141a\"><div class=\"gb-layout-column-wrap gb-block-layout-column-gap-2 gb-is-responsive-column\">\n<div class=\"wp-block-genesis-blocks-gb-column dbio-icons-use-case gb-block-layout-column\"><div class=\"gb-block-layout-column-inner\" style=\"padding-top:3em;padding-bottom:1em\">\n<h2 class=\"has-text-align-center has-text-color\" id=\"h-popular-use-cases\" style=\"color:#fefefe\">Popular use cases<\/h2>\n<\/div><\/div>\n<\/div><\/div>\n\n\n\n<div class=\"wp-block-genesis-blocks-gb-columns gb-layout-columns-3 gb-3-col-equal gb-has-custom-background-color gb-columns-center alignfull\" style=\"padding-right:1.5px;padding-left:1.5px;background-color:#14141a\"><div class=\"gb-layout-column-wrap gb-block-layout-column-gap-2 gb-is-responsive-column\" style=\"max-width:1000px\">\n<div class=\"wp-block-genesis-blocks-gb-column gb-block-layout-column\"><div class=\"gb-block-layout-column-inner\">\n<div class=\"wp-block-image dbio-icons-use-case\"><figure class=\"aligncenter size-large\"><img  src=\"https:\/\/dolby.io\/wp-content\/up